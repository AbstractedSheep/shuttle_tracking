<!DOCTYPE html>
<html>
<head>
  <title>Tracking</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div id="user_nav">  
		<% if current_user %>
			<p><%= link_to "Logout", logout_path %></p>
		<% else %>
			<p><%= link_to "Register", new_user_path %>|<%= link_to "Login", login_path %></p>
		<% end %>
	</div>  
<%= yield %>

</body>
</html>
